{
  "name": "ai-processing-service",
  "version": "1.0.0",
  "description": "AI Processing Service for Nanopore PDF Processing and Data Extraction",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "migrate": "tsx src/database/migrate.ts",
    "health": "curl http://localhost:3003/health",
    "docker:build": "docker build -t ai-processing-service .",
    "docker:run": "docker run -p 3003:3003 ai-processing-service"
  },
  "keywords": [
    "nanopore",
    "ai-processing",
    "pdf-processing",
    "microservice",
    "typescript",
    "ollama",
    "qdrant"
  ],
  "author": "Nanopore Tracking Team",
  "license": "MIT",
  "dependencies": {
    "@trpc/server": "^10.45.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "kysely": "^0.26.3",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "qdrant": "^1.7.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}