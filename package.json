{
  "name": "@app/monorepo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "description": "Monorepo scaffold",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "preview": "turbo run preview",
    "lint": "eslint . --max-warnings=0",
    "typecheck": "tsc -b",
    "test": "turbo run test",
    "test:e2e": "pnpm --filter @app/web test:e2e",
    "test:e2e:ui": "pnpm --filter @app/web test:e2e:ui",
    "test:e2e:debug": "pnpm --filter @app/web test:e2e:debug",
    "test:e2e:report": "pnpm --filter @app/web test:e2e:report",
    "fix": "pnpm run fix:tsconfig && pnpm run fix:eslint && pnpm run fix:prettier",
    "fix:prettier": "prettier --write .",
    "fix:eslint": "eslint . --fix",
    "fix:tsconfig": "monorepo-typescript-references fix",
    "dev-loop": "./scripts/dev-loop.sh",
    "dev-loop:check": "./scripts/dev-loop.sh check",
    "dev-loop:test": "./scripts/dev-loop.sh test",
    "dev-loop:clean": "./scripts/dev-loop.sh clean",
    "devlooper": "./scripts/devlooper-integration.sh",
    "devlooper:setup": "./scripts/devlooper-integration.sh setup",
    "devlooper:presets": "./scripts/devlooper-integration.sh presets"
  },
  "dependencies": {
    "@types/file-saver": "^2.0.7",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "recharts": "^3.0.0"
  },
  "devDependencies": {
    "@ocavue/eslint-config": "^3.0.0",
    "@ocavue/tsconfig": "^0.3.7",
    "eslint": "^9.26.0",
    "monorepo-typescript-references": "^1.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "turbo": "^2.5.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "pnpm": {
    "overrides": {},
    "ignoredBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "bufferutil",
      "core-js",
      "esbuild",
      "prisma",
      "protobufjs",
      "sharp",
      "unrs-resolver"
    ]
  }
}
